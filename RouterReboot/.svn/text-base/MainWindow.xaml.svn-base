<Window x:Class="RouterReboot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RouterReboot" Height="141" Width="244">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="tbIp" VerticalAlignment="Top" Width="120" Text="192.168.0.1" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" Name="tbUserName" VerticalAlignment="Top" Width="120" Text="admin" />
        <Button Content="Reboot now" Height="23" HorizontalAlignment="Left" Margin="138,70,0,0" Name="btnReboot" VerticalAlignment="Top" Width="75" Click="btnReboot_Click" IsDefault="True" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="12,70,0,0" Name="pbPassword" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
